<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.journey.mapper.ConsultMapper">
  <insert id="insertConsult">
  	insert into consult values(#{userName},#{age},#{time},#{content},#{contact})
  </insert>
  <delete id="deleteConsult">
  	delete from consult where username=#{userName} and age=#{age}
  </delete>
</mapper>