<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.journey.mapper.TestMapper">
  <insert id="insertTest">
  	insert into test values(#{num},#{userName},#{result})
  </insert>
  <delete id="deleteTest">
  	delete from test where num=#{num} and userName=#{userName}
  </delete>
</mapper>